<?xml version='1.0' encoding='UTF-8'?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <template class="EmbedWidget" parent="GtkGrid">
    <child>
      <object class="GtkLabel" id="lbl_dest">
        <property name="label" translatable="yes">Output Filename:</property>
        <layout>
          <property name="column">0</property>
          <property name="row">4</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="GtkLabel" id="lbl_tag">
        <property name="label" translatable="yes">Sound Tag</property>
        <layout>
          <property name="column">0</property>
          <property name="row">3</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="GtkButton">
        <layout>
          <property name="column">1</property>
          <property name="column-span">1</property>
          <property name="row">1</property>
          <property name="row-span">1</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="GtkLabel" id="lbl_audio">
        <property name="label" translatable="yes">Audio Filename:</property>
        <layout>
          <property name="column">0</property>
          <property name="row">2</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="GtkButton">
        <layout>
          <property name="column">1</property>
          <property name="column-span">1</property>
          <property name="row">2</property>
          <property name="row-span">1</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="GtkLabel" id="lbl_image">
        <property name="label" translatable="yes">Image Filename:</property>
        <layout>
          <property name="column">0</property>
          <property name="row">1</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="GtkLabel" id="lbl_embed">
        <property name="label" translatable="yes">Embed</property>
        <layout>
          <property name="column">1</property>
          <property name="row">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="GtkEntry" id="e_tag">
        <property name="focusable">1</property>
        <layout>
          <property name="column">1</property>
          <property name="row">3</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="GtkEntry" id="e_dest">
        <property name="focusable">1</property>
        <layout>
          <property name="column">1</property>
          <property name="row">4</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="GtkButton" id="btn_embed">
        <property name="focusable">1</property>
        <property name="label" translatable="yes">Embed</property>
        <property name="receives-default">1</property>
        <layout>
          <property name="column">2</property>
          <property name="row">4</property>
        </layout>
      </object>
    </child>
  </template>
</interface>
